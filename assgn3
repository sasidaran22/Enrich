#include <stdio.h>
 
long factorial(int);
 
int main()
{
   int i, j;
 
   for (i = 0; i < 6; i++)
   {
      
 
      for (j = 0 ; j <= i; j++){
         printf("%ld   ",factorial(i)/(factorial(j)*factorial(i-j)));
      }
      printf("\n");
   }
 
   return 0;
}
 
long factorial(int n)
{
   int c;
   long result = 1;
 
   for (c = 1; c <= n; c++)
         result = result*c;
 
   return result;
}
